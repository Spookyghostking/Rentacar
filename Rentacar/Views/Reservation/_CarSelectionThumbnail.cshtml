@model IEnumerable<Rentacar.DataModels.CarImageDataModel>

@if (Model.Count() > 0)
{
    <div class="card-img-top">
        <img src="@Model.First().Url" class="card-img-top" alt="Image of @Model.First().Car.CodeName">
    </div>
}
else
{
    <div class="card-img-top">
        <img src="/img/Car/default.png" class="card-img-top" alt="No image available">
    </div>
}